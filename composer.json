{
  "name": "ingenerator/behat-support",
  "description": "Various support functions and classes for behat testing",
  "homepage": "https://github.com/ingenerator/behat-support",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Andrew Coulton",
      "email": "andrew@ingenerator.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "behat/behat": "^3.11",
    "ext-json": "*",
    "symfony/http-client": "^5.4 || ^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.5"
  },
  "support": {
    "source": "https://github.com/ingenerator/behat-support/",
    "issues": "https://github.com/ingenerator/behat-support/issues"
  },
  "autoload": {
    "psr-4": {
      "Ingenerator\\BehatSupport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\Ingenerator\\BehatSupport\\": "test/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
